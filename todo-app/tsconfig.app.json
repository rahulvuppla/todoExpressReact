{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "target": "ES6",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "moduleResolution": "node",
    "paths": {
      "@components/*": ["components/*"],
      "@redux/*": ["redux/*"],
      "@types/*": ["types/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
